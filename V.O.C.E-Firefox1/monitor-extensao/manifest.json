{
  "manifest_version": 2,
  "name": "Monitor de Atividade V.O.C.E",
  "version": "1.1",
  "description": "Monitora a atividade de navegação para o TCC.",
  "permissions": [
    "tabs",
    "webNavigation",
    "storage",
    "alarms",
    "<all_urls>",
    "http://localhost:3000/"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "browser_action": {
    "default_title": "Configurações do Monitor"
  }
}